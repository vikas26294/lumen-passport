{
    "name": "dusterio/lumen-passport",
    "type": "library",
    "description": "Making Laravel Passport work with Lumen",
    "keywords": ["php","lumen","laravel passport"],
    "homepage": "https://github.com/vikas26294/lumen-passport",
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "laravel/passport",
                "version": "dev-dev-prod",
                "source": {
                    "url": "https://github.com/vikas26294/passport.git",
                    "type": "git",
                    "reference": "v4.0.8-dev"
                }
            }
        }
    ],
    "authors": [
        {
            "name": "Denis Mysenko",
            "email": "denis@mysenko.com",
            "homepage": "https://www.mysenko.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "~5.3|~5.4",
        "illuminate/support": "~5.3|~5.4",
        "laravel/passport": "dev-dev-prod"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Dusterio\\LumenPassport\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
